{
  "name": "deku",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.3.0",
  "description": "ðŸ’° Web application to control your income",
  "author": {
    "name": "Rafael Curi",
    "email": "rafael@curi.dev.br",
    "url": "https://curi.dev.br"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/rafinhacuri",
  "homepage": "https://deku.curi.dev.br",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelcuri/deku.git"
  },
  "bugs": "https://github.com/rafaelcuri/deku/issues",
  "keywords": ["money", "control", "vue", "nuxt", "typescript", "go", "golang"],
  "scripts": {
    "build": "nuxt build",
    "app:dev": "nuxt dev",
    "app:build": "nuxt build",
    "api:dev": "cd api && ~/go/bin/air",
    "dev": "concurrently -n APP,API -c green,blue \"bun app:dev\" \"bun api:dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "prerelease": "bun lint && bun typecheck",
    "docker:build": "docker build --platform=linux/amd64 -t mizle/deku:latest -t mizle/deku:$(node -p \"require('./package.json').version\") .",
    "docker:push": "docker push mizle/deku:latest && docker push mizle/deku:$(node -p \"require('./package.json').version\")",
    "release": "bun prerelease && bumpp && bun docker:build && bun docker:push"
  },
  "dependencies": {
    "@iconify-json/lucide": "^1.2.78",
    "@nuxt/ui": "4.2.1",
    "@nuxtjs/i18n": "^10.2.1",
    "@nuxtjs/seo": "^3.2.2",
    "@vueuse/nuxt": "^14.1.0",
    "nuxt": "^4.2.1",
    "nuxt-security": "^2.5.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@dethdkn/eslint-config": "^4.0.3",
    "bumpp": "^10.3.2",
    "eslint": "^9.39.1",
    "vue-tsc": "^3.1.5"
  }
}
