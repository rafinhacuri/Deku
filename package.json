{
  "name": "deku",
  "type": "module",
  "version": "1.0.4",
  "private": true,
  "packageManager": "bun@1.3.0",
  "description": "ðŸ’° Web application to control your income",
  "author": {
    "name": "Rafael Curi",
    "email": "rafael@curi.dev.br",
    "url": "https://curi.dev.br"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/rafinhacuri",
  "homepage": "https://deku.curi.dev.br",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rafaelcuri/deku.git"
  },
  "bugs": "https://github.com/rafaelcuri/deku/issues",
  "keywords": ["money", "control", "vue", "nuxt", "typescript", "go", "golang"],
  "scripts": {
    "build": "nuxt build",
    "app:dev": "nuxt dev",
    "app:build": "nuxt build",
    "api:dev": "cd api && ~/go/bin/air",
    "dev": "concurrently -n APP,API -c green,blue \"bun app:dev\" \"bun api:dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "fmt": "oxfmt --check",
    "fmt:fix": "oxfmt",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --type-aware --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "gentypes": "bun run --cwd api/cmd go run typescript.go && eslint shared/types/goServer.d.ts --fix",
    "prerelease": "bun lint && bun typecheck",
    "docker:build": "docker build --platform=linux/amd64 -t mizle/deku:latest -t mizle/deku:$(node -p \"require('./package.json').version\") .",
    "docker:push": "docker push mizle/deku:latest && docker push mizle/deku:$(node -p \"require('./package.json').version\")",
    "release": "bun prerelease && bumpp && bun docker:build && bun docker:push"
  },
  "dependencies": {
    "@iconify-json/circle-flags": "^1.2.10",
    "@iconify-json/lucide": "^1.2.80",
    "@nuxt/image": "2.0.0",
    "@nuxt/ui": "4.2.1",
    "@nuxtjs/i18n": "^10.2.1",
    "@nuxtjs/seo": "^3.2.2",
    "@vueuse/nuxt": "^14.1.0",
    "nuxt": "^4.2.2",
    "nuxt-aos": "^1.2.5",
    "nuxt-security": "^2.5.0",
    "ofetch": "^1.5.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@dethdkn/ox-config": "^1.0.5",
    "bumpp": "^10.3.2",
    "vitepress": "^1.6.4",
    "vue-tsc": "^3.1.8"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "@tailwindcss/oxide",
    "unrs-resolver"
  ]
}
